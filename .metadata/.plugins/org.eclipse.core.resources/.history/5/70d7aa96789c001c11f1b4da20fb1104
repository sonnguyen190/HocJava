package main;

public class baitap {

	public static void main(String[] args) {
		byte so1 = 1;
		short so2 = 2;
		int so3 = 3;
		long so4= 4;
		float floatt = 5.1f;
		double doublee = 6.2;
		boolean bolem = true;
		char cha = 's';
		String chuoi = "dhgjksd";
		
		
		System.out.println(so1);
		System.out.println(so2);
		System.out.println(so3);
		System.out.println(so4);
		System.out.println(floatt);
		System.out.println(doublee);
		System.out.println(bolem);
		System.out.println(cha);
		System.out.println(chuoi);
		
	}
	public static void phuongThuc2(String[] args) {
		String ame = "Nguyen Van lam Son";
		System.out.println(ame);
	}
	
	

}
